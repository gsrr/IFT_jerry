# Pathogen
mkdir -p ~/.vim/autoload ~/.vim/bundle && curl -LSso ~/.vim/autoload/pathogen.vim https://tpo.pe/pathogen.vim

# plugin:ctrlp.vim
git clone https://github.com/kien/ctrlp.vim ~/.vim/bundle/ctrlp.vim

# jedi-vim
pip install jedi
git clone https://github.com/davidhalter/jedi-vim ~/.vim/bundle/jedi-vim

# nerdtree
git clone https://github.com/scrooloose/nerdtree ~/.vim/bundle/nerdtree

# supertab
git clone https://github.com/ervandew/supertab ~/.vim/bundle/supertab

# syntastic
git clone https://github.com/scrooloose/syntastic ~/.vim/bundle/syntastic

# tagbar
yum -y install ctags
git clone https://github.com/majutsushi/tagbar ~/.vim/bundle/tagbar

# vim-airline
git clone https://github.com/vim-airline/vim-airline ~/.vim/bundle/vim-airline

# vim-fugitive
git clone https://github.com/tpope/vim-fugitive ~/.vim/bundle/vim-fugitive

# vim-powerline
pip install powerline
git clone https://github.com/Lokaltog/powerline ~/.vim/bundle/powerline

# auto-close
git clone https://github.com/Townk/vim-autoclose

# vim-monokai
git clone https://github.com/sickill/vim-monokai

# minibufexpl.vim
git clone https://github.com/fholgado/minibufexpl.vim

# snipmate
git clone https://github.com/tomtom/tlib_vim.git
git clone https://github.com/MarcWeber/vim-addon-mw-utils.git
git clone https://github.com/garbas/vim-snipmate.git
git clone https://github.com/honza/vim-snippets.git
